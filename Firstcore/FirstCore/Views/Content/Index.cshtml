@model ContentViewModel
@{
    ViewData["Title"] = "Index";
}
<div>
    <table>
        <thead>
            <tr>
                <th><inppt type="checkbox" /></th>
                <th>序号</th>
                <th>标题</th>
                <th>内容</th>
                <th>添加时间</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Contents)
            {
            <tr>
                <td>
                    <input type="checkbox" class="done-checkbox">
                </td>
                <td>@item.Id</td>
                <td>@item.title</td>
                <td>@item.content</td>
                <td>@item.add_time</td>
            </tr>
            }
        </tbody>
    </table>
</div>

